.container{
    padding-bottom: 100rpx; 
}
.top{
  height: 90rpx;
  padding: 0 40rpx;
}
.top>image{
  width: 60rpx;
  height: 60rpx;
}
.cell-top image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 18rpx;
}
.cell-bottom{
  padding-left: 48rpx;
}

.priceList{
  padding: 25rpx 0;
  border-top: 1rpx solid #ebebeb;
  border-bottom: 1rpx solid #ebebeb;
}
.priceList>view{
  font-size: 24rpx;
  color: #898989;
  padding: 0 32rpx;
  line-height: 40rpx;

}
.priceList>view>text:first-of-type{
  color: #666;
}
.total{
  height: 72rpx;
  padding: 0 32rpx;
  font-size: 26rpx;
  color: #898989;
  font-weight: 700;
}
.total>view>text:first-of-type{
  color: #666;
}

.bottom-info{
  padding: 0 40rpx 30rpx;
  line-height: 54rpx;
  font-size: 24rpx;
  color: #666;
}
.bottom-info .title{
  line-height: 84rpx;
  border-bottom: 1rpx solid #ebebeb;
  font-size: 28rpx;
  font-weight: 700;
  margin-bottom: 30rpx;
}
.end{

  /* justify-content: space-between; */
  /* padding: 0 100rpx; */
  padding: 0 25rpx;
  justify-content: flex-end;
}

.end-word{
  font-size: 34rpx;
  color: #000;
  font-weight: 700;
}
.end-btn{
  margin-left: 30rpx;
  padding: 0 30rpx !important
}

.goods-item .info .name{
  width: 380rpx;
}

.express-box{
  width: 100%;
  border-radius: 10rpx 10rpx 0 0 ;

  background-color: #fff;
}
.express-top{
  height: 240rpx;
  border-bottom: 10rpx solid #ebebeb;
  padding:0 0 30rpx 70rpx;
  
}
.express-title{
  font-weight: 700;
  font-size: 30rpx;

}
.express-title>image{
  width: 25rpx;
  height: 25rpx;
  padding: 40rpx;
}
.express-info{
  font-size: 24rpx;
  color: #666;
  line-height: 36rpx;
}


.express-detail{
  padding: 20rpx 0 20rpx 0rpx;
  overflow: hidden;
}
.express-detail>.item{
  padding: 25rpx 0;
  position: relative;
  padding-left: 145rpx;

}
.item .radio{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #e6e6e6;
    position: absolute;
  left: 90rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 9;
}
.line{
  width: 2rpx;
  background-color: #e6e6e6;
  height: 5000rpx;
  position: absolute;
  left: 98rpx;
  top: 50%;
  z-index: 8;
}
.item .radio.active{
  background-color: #583ec4;
  border: 10rpx solid #f8cfd2;
  box-sizing: content-box;
  left: 80rpx;
}
.item .content{
  font-size: 24rpx;
  color: #666;
  margin-bottom: 22rpx;
}
.item .time{
  font-size:24rpx;
  color: #999;
}
.item .content.theme-word{
  color: #583ec4;
}

.time{
  font-size: 24rpx;
}
.end{
  border-top: 0;
}

.end-paid{
  padding: 0 25rpx;
}